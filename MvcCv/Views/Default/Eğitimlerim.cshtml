@using MvcCv.Models.Entity
@model List<Tbl_Egitim>

<div class="resume-section-content">
    <h2 class="mb-5"><span style="color:darkorange;">Təhsİl</span> <span style="color:black;">HəyatIm</span> </h2>
    @foreach (var x in Model)
    {
    <div class="d-flex flex-column flex-md-row justify-content-between mb-5">
        <div class="flex-grow-1">
            <h3 class="mb-0" style="color:purple">@x.AltBaslik1</h3>
            <div class="subheading mb-3" style="color:purple">@x.AltBaslik2</div>
            <p>@x.GNO</p>
        </div>
        <div class="flex-shrink-0"><span class="text-primary">@x.Tarih</span></div>
    </div>
    }
</div>
